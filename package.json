{
  "name": "lfa-corporate-landing",
  "private": false,
  "description": "LF Академия — Корпоративное обучение",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.x",
    "autoprefixer": "9.8.x",
    "browser-sync": "2.26.x",
    "gulp": "4.0.x",
    "gulp-csso": "^4.0.1",
    "gulp-header": "^2.0.9",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "4.1.0",
